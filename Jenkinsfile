pipeline {
  agent any

  stages {
    stage ('Build') {
      steps {
        echo 'Hi, welcome to pipeline demo...'
     }
   }
    stage ('test') {
      steps {
        sh "echo Run a command"
      }
    
    }
    stage ('deploy') {
      steps {
        echo 'Thanks for using Jenkins Pipeline'
      }
    
    }
  }
}
